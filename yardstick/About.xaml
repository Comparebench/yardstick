<Window x:Class="yardstick.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:yardstick"
        mc:Ignorable="d"
        Title="About" Height="120" Width="250">
	<StackPanel>
		<TextBlock Text="Comparebench Yardstick v0.0" Width="220" />
		<TextBlock Width="218">
		    <Hyperlink NavigateUri="https://comparebench.com" RequestNavigate="Hyperlink_RequestNavigate">
                https://comparebench.com
            </Hyperlink>
            </TextBlock>
		<TextBlock Width="220">
		    <Hyperlink NavigateUri="https://twitter.com/comparebench" RequestNavigate="Hyperlink_RequestNavigate">
                @comparebench
            </Hyperlink>
		</TextBlock>
		<Button  Click="btnClose" Height="20" Content="Close" Width="60" />
	</StackPanel>
</Window>
