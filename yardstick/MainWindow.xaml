<Window x:Class="yardstick.MainWindow" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:yardstick"
        mc:Ignorable="d"
        Title="Comparebench Yardstick" Height="450" Width="800" d:DataContext="{d:DesignInstance }">
    <Grid>
        <TextBlock Name="textBlock1" TextWrapping="Wrap" Text="{Binding ModelName,RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=Window}}">
        </TextBlock>
    </Grid>
</Window>
